<%- layout('layouts/boilerplate') %>
<h1>New Campground</h1>
<form action="/campgrounds/new" method="POST">
    <div>
        <label for="title">
            Title:
        </label>
        <input type="text" id="title" name="camp[title]" placeholder="Title">
    </div>
    <div>
        <label for="location">
            Location:
        </label>
        <input type="text" id="location" name="camp[location]" placeholder="Location">
    </div>
    <button>Add Campground</button>
</form>

<footer>
    <p>
        <a href="/campgrounds">All Campgrounds</a>
    </p>
</footer>